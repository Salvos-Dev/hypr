{
    "manifest_version": 3,
    "name": "Void Start",
    "version": "1.0",
    "description": "Allows for custom startpage on both new tab and new window.",
    "chrome_url_overrides": {
        "newtab": "startpage.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ]
}
